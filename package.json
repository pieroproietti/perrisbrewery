{
  "name": "perrisbrewery",
  "description": "Create manpages, insert preinst, postinst, prerm and postrm",
  "version": "26.1.17",
  "type": "module",
  "author": "Piero Proietti",
  "bin": {
    "pb": "./bin/run.js"
  },
  "bugs": "https://github.com/pieroproietti/perrisbrewery/issues",
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-autocomplete": "^3.2.39",
    "@oclif/plugin-help": "^6.2.36",
    "@oclif/plugin-version": "^2.2.36",
    "@oclif/test": "^4.1.15",
    "chalk": "^5.6.2",
    "fs-extra": "^11.3.3",
    "js-yaml": "^4.1.1",
    "mustache": "^4.2.0",
    "rehype-document": "^7.0.3",
    "rehype-format": "^5.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-man": "^9.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "to-vfile": "^8.0.0",
    "tslib": "2.8.1",
    "typescript": "^5.9.3",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.6",
    "@types/node": "^25.0.9",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-oclif": "^6.0.128",
    "eslint-config-prettier": "^10.1.8",
    "globby": "^16.1.0",
    "nps": "^5.10.0",
    "oclif": "^4.22.61",
    "ts-node": "^10.9.2"
  },
  "dirname": "pb",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/perrisbrewery"
  ],
  "homepage": "https://penguins-eggs.net",
  "keywords": [
    "oclif",
    "penguins-eggs"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "oclif": {
    "bin": "pb",
    "commands": "./dist/commands",
    "dirname": "pb",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "pieroproietti/perrisbrewery",
  "scripts": {
    "build": "tsc -p .",
    "lint-and-fix": "eslint . --ext .ts --fix --config .eslintrc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "manifest": "oclif manifest",
    "readme": "oclif readme",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif readme",
    "test": "echo NO TESTS",
    "version": "oclif readme && git add README.md"
  },
  "publishConfig": {
    "not_used_registry": "http://localhost:4873",
    "registry": "https://registry.npmjs.org"
  },
  "types": "lib/index.d.ts"
}