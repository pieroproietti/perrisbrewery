{
  "name": "perrisbrewery",
  "description": "insert manpages, preinst, postinst, prerm postrm scripts",
  "version": "9.5.9",
  "author": "Piero Proietti",
  "bin": {
    "pb": "./bin/run.js"
  },
  "bugs": "https://github.com/pieroproietti/perrisbrewery/issues",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-autocomplete": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-version": "^2",
    "@oclif/plugin-warn-if-update-available": "^3",
    "@oclif/test": "^4",
    "chalk": "^4.1.0",
    "fs-extra": "^8.1",    
    "inquirer": "8.2.4",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "rehype-document": "^5.1.0",
    "rehype-format": "^3.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-man": "^7.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "shelljs": "0.8.5",
    "to-vfile": "6.1.0",
    "tslib":"2.6.2",
    "typescript": "^5.4.5",
    "unified": "^9.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.12",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "globby": "^11.1",
    "nps": "^5.10.0",
    "ts-node": "^10.9.2"
  },
  "dirname": "pb",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/perrisbrewery"
  ],
  "homepage": "https://github.com/pieroproietti/perrisbrewery",
  "keywords": [
    "oclif",
    "penguins-eggs"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "oclif": {
    "bin": "pb",
    "commands": "./lib/commands",
    "dirname": "pb",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "pieroproietti/perrisbrewery",
  "scripts": {
    "build": "tsc -p .",
    "lint": "nps lint",
    "manifest": "oclif manifest",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "_prepack": "rm -rf lib && tsc -b && oclif readme",
    "prepack": "rm -rf lib && tsc -b",
    "test": "echo NO TESTS",
    "version": "oclif readme && git add README.md"
  },
  "publishConfig": {
    "not_used_registry": "http://localhost:4873",
    "registry": "https://registry.npmjs.org"
  },
  "types": "lib/index.d.ts"
}