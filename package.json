{
  "name": "perrisbrewery",
  "description": "insert preinst, postinst, prerm postrm scripts",
  "version": "9.3.13",
  "author": "Piero Proietti",
  "bin": {
    "pb": "bin/run"
  },
  "bugs": "https://github.com/pieroproietti/perrisbrewery/issues",
  "dependencies": {
    "@oclif/core": "^1.21.0",
    "@oclif/plugin-autocomplete": "^1.3.3",
    "@oclif/plugin-help": "^5.1.16",
    "@oclif/plugin-not-found": "^2.3.6",
    "@oclif/plugin-version": "^1.0.4",
    "@oclif/plugin-warn-if-update-available": "^2.0.9",
    "chalk": "^4.1.0",
    "inquirer": "8.2.4",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "rehype-document": "^5.1.0",
    "rehype-format": "^3.1.0",
    "rehype-stringify": "^8.0.0",
    "remark-man": "^7.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "shelljs": "0.8.5",
    "to-vfile": "6.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.4",
    "unified": "^9.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mustache": "^4.1.3",
    "@types/node": "^18.11.18",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^7",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "install": "^0.13.0",
    "oclif-pnpm": "3.4.2-12",
    "ts-node": "^10.8.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/plugin": "^5.0.0",
    "typescript": "^4.6.0"
  },
  "dirname": "pb",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/perrisbrewery"
  ],
  "homepage": "https://github.com/pieroproietti/perrisbrewery",
  "keywords": [
    "oclif",
    "penguins-eggs"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "oclif": {
    "bin": "pb",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-version"
    ],
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "pieroproietti/perrisbrewery",
  "scripts": {
    "build": "tsc -p .",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif readme",
    "test": "echo NO TESTS",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
